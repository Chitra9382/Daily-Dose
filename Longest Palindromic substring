class Solution:
 def Longestpalindrome(self,s:str):
    def expandFromCentre(s,l,r):
        max_len=0
        substring=''
        while l>=0 and r<(len(s)) and s[l]==s[r]:
               if r-l+1 > max_len:
                   r-l+1=max_len
                   substring=s[l:r+1]
                   l-=1
                   r+=1
         return substring
    result=''
    for i in range (len(s)):
        odd=expandFromCentre(s,i,i)
        even=expandFromCentre(s,i,i+1)
        if len(odd) > len(result):
            result=odd
        if len(even)>len(result):
            result=even
    return result
               
